            <xml-property name="queryText"><![CDATA[select emp_id,
       name,
       alias_name,
       department,
       group_name,
       currency,
       ifnull(bonus, 0) as bonus,
       gross_salary + ifnull(bonus, 0) + ifnull(thr, 0) as total_compansation,
       new_salary,
       old_salary,
       new_salary - old_salary as nominal_increase,
       (new_salary - old_salary) / old_salary as percentage_increase
from(
select employee_payment.emp_id,
       employee.name,
       employee.alias_name,
       employee.department,
       employee_group.name as group_name,
       employee_salary.currency,
       sum(
          case
            when employee_payment.currency = 'USD' then tax_mgross_salary / employee_payment.rate
            when employee_payment.currency = 'IDR' then tax_mgross_salary
          end
       ) as gross_salary,
       (select sum(ifnull(case
            when currency = 'USD' then thr_amount / rate
            when currency = 'IDR' then thr_amount
          end,0))
        from employee_thr
        where emp_id = employee_payment.emp_id and
              year = ?
        ) as thr,
       (select sum(ifnull(case
            when currency = 'USD' then bonus / rate
            when currency = 'IDR' then bonus
          end,0))
        from employee_bonus
        where emp_id = employee_payment.emp_id and
              year = ?
        ) as bonus,
       (select ifnull(sum(case
            when currency = 'USD' then amount / rate
            when currency = 'IDR' then amount
          end),0)
        from employee_deduction
        where emp_id = employee_payment.emp_id and
              year = ?
        ) as emp_deduction,
       (select sum(ifnull(case
            when employee_payment.currency = 'USD' then amount / employee_payment.rate
            when employee_payment.currency = 'IDR' then amount
          end,0))
        from payment_jamsostek
        where payment_id in (select id
                             from employee_payment
                             where emp_id = employee_payment.emp_id and
                                   year = 2014) and
              charge_to = 2) as emp_jht,
        employee_salary.amount as new_salary,
        (select a.amount
         from employee_salary a
         where a.nett_gross = employee_salary.nett_gross and
               a.end_date = STR_TO_DATE(CONCAT(?,',12,31'), '%Y,%m,%d')/*(select max(b.end_date)
                             from employee_salary b
                             where b.end_date = STR_TO_DATE(CONCAT(2015-1,',12,31'), '%Y,%m,%d') and
                                   a.emp_id = b.emp_id)*/ and
               a.emp_id = employee_payment.emp_id
        ) as old_salary
from employee_payment, employee_salary, employee, master_data as employee_group
where year = ? and
      employee_payment.emp_id = employee_salary.emp_id and
      STR_TO_DATE(CONCAT(?+1,',1,1'), '%Y,%m,%d') between employee_salary.start_date and employee_salary.end_date and
      employee_payment.emp_id = employee.id and
      employee.record_status = 1 and
      employee.emp_group = employee_group.value and 
      employee_group.type_id = 21 and
      ((employee.id = ? and ? <> 0) or
      (? = 0))
group by employee_payment.emp_id,
         employee.name,
         employee.alias_name,
         employee_group.name,
         employee_salary.currency,
         employee.department
) as a]]></xml-property>
