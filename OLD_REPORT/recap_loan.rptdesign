            <xml-property name="queryText"><![CDATA[select emp_id,
	   name,
	   nik,
	   amount,
	   ssek_interest,
	   installment,
	   installment_amount,
	   bank_name,
	   term_of_loan,
	   min(balance),
	   max(total_payment),
	   (select (gross_salary - tax) * 10 as nett_salary
		from(
			select salary + JKM + JKK + allowance - JHT - ((salary + JKM + JKK + allowance) * a.percentage / 100 ) as tax_nett_salary,
				   ((salary + JKM + JKK + allowance - JHT - ((salary + JKM + JKK + allowance) * a.percentage / 100 )) * 12) - ptkp_amount as yearly_tax_nett_salary,
			       depnaker_pph21_detail.percentage,
			       (case
			       		when ((salary + JKM + JKK + allowance - JHT - ((salary + JKM + JKK + allowance) * a.percentage / 100 )) * 12) - ptkp_amount  <= depnaker_pph21_detail.max_amount then (((((salary + JKM + JKK + allowance - JHT - ((salary + JKM + JKK + allowance) * a.percentage / 100 )) * 12) - ptkp_amount) - depnaker_pph21_detail.min_amount) * depnaker_pph21_detail.percentage / 100) / 12
			       		else ((depnaker_pph21_detail.max_amount - depnaker_pph21_detail.min_amount)  * depnaker_pph21_detail.percentage / 100) / 12
			       	end ) as tax,
			       	salary + allowance - JHT as gross_salary 
			from (
				select ifnull(employee_payment.salary, 0) as salary,
					   ifnull(employee_payment.ptkp_amount, 0) as ptkp_amount,
					   ifnull(payment_allowance.amount, 0) as allowance,
					   ifnull(depnaker_position_allowance.percentage, 0) as percentage,
					   ifnull(MAX(IF(payment_jamsostek.jamsostek_type = 0, payment_jamsostek.amount, NULL)), 0) AS JHT,
					   ifnull(MAX(IF(payment_jamsostek.jamsostek_type = 1, payment_jamsostek.amount, NULL)), 0) AS JKM,
					   ifnull(MAX(IF(payment_jamsostek.jamsostek_type = 2, payment_jamsostek.amount, NULL)), 0) AS JKK
				from employee_payment join employee_loan on employee_payment.emp_id = employee_loan.emp_id 
										   					and str_to_date(concat(employee_loan.start_month,'-',employee_loan.start_year),'%m-%Y') <= str_to_date(concat(?,'-',?),'%m-%Y')
										   					and str_to_date(concat(employee_loan.start_month,'-',employee_loan.start_year),'%m-%Y') = str_to_date(concat(employee_payment.month,'-',employee_payment.year),'%m-%Y')
									  left outer join payment_jamsostek on employee_payment.id = payment_jamsostek.payment_id
									  									   and (payment_jamsostek.jamsostek_type in (1, 2) or
									  									   (payment_jamsostek.jamsostek_type = 0 and payment_jamsostek.charge_to = 2))
									  left outer join payment_allowance on employee_payment.id = payment_allowance.payment_id
									  									   and payment_allowance.description like '%Jabatan%'
									  									   and payment_allowance.nature = 'M'
									  left outer join depnaker_position_allowance on str_to_date(concat('25-',employee_loan.start_month,'-',employee_loan.start_year),'%d-%m-%Y') >= depnaker_position_allowance.start_date
					  																 and str_to_date(concat('25-',employee_loan.start_month,'-',employee_loan.start_year),'%d-%m-%Y') <= depnaker_position_allowance.end_date
				where employee_payment.emp_id = 1
				group by employee_payment.salary,
					   payment_allowance.amount,
					   depnaker_position_allowance.percentage
			) as a, depnaker_pph21, depnaker_pph21_detail
			where str_to_date(concat('25-', '01','-',2013),'%d-%m-%Y') >= depnaker_pph21.start_date
			      and str_to_date(concat('25-', '01','-',2013),'%d-%m-%Y') <= depnaker_pph21.end_date
			      and depnaker_pph21.id = depnaker_pph21_detail.depnaker_pph21_id
			      and ((salary + JKM + JKK + allowance - JHT - ((salary + JKM + JKK + allowance) * a.percentage / 100 )) * 12) - ptkp_amount > depnaker_pph21_detail.min_amount
		) as b) as budget
from(
	select employee_loan.emp_id,
		   employee.name,
		   employee.nik,
		   employee_loan.amount,
		   employee_loan.ssek_interest,
		   employee_loan.installment,
		   employee_loan.installment_amount,
		   employee_loan.bank_name,
		   DATE_ADD(str_to_date(concat('31-',employee_loan.start_month,'-',employee_loan.start_year),'%d-%m-%Y'), INTERVAL employee_loan.installment-1 MONTH) as term_of_loan,
	       employee_loan_payment.total_monthly_loan - employee_loan_payment.payment_amount as balance,
	       (select sum(loan_payment.payment_amount)
	       	from employee_loan_payment as loan_payment
	       	where loan_payment.loan_id = employee_loan_payment.loan_id
	       		  and str_to_date(concat(loan_payment.month,'-',loan_payment.year),'%m-%Y') <= str_to_date(concat(employee_loan_payment.month,'-',employee_loan_payment.year),'%m-%Y')) as total_payment
	from employee_loan, employee, employee_loan_payment
	where str_to_date(concat(employee_loan.start_month,'-',employee_loan.start_year),'%m-%Y') <= str_to_date(concat(?,'-',?),'%m-%Y')
		  and DATE_ADD(str_to_date(concat('31-',employee_loan.start_month,'-',employee_loan.start_year),'%d-%m-%Y'), INTERVAL employee_loan.installment-1 MONTH) >= str_to_date(concat(?,'-',?),'%m-%Y')
		  and employee.id = employee_loan.emp_id
		  and employee_loan.id = employee_loan_payment.loan_id
		  and str_to_date(concat(employee_loan_payment.month,'-',employee_loan_payment.year),'%m-%Y') <= str_to_date(concat(?,'-',?),'%m-%Y')
) a
group by emp_id,
	   name,
	   amount,
	   ssek_interest,
	   installment,
	   installment_amount,
	   term_of_loan,
	   bank_name]]></xml-property>
