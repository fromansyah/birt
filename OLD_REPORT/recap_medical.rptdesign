            <xml-property name="queryText"><![CDATA[select employee.id,
	   employee.name,
	   employee.nik,
	   employee_medical.type,
	   (case
	   		when employee_medical.type = 0 then 'No Dependents'
	   		when employee_medical.type = 1 then 'One Dependents'
	   		when employee_medical.type = 2 then 'Two Dependents'
	   		when employee_medical.type = 3 then 'Three Dependents'
	    end ) as dependents,
	   employee_medical.amount,
	   ifnull((select sum(approved_amount)
		from medical_claim
		where employee.id = medical_claim.emp_id
		      and medical_claim.status in (1, 3)
		      and medical_claim.year = ? 
		      and medical_claim.month <= ? ), 0) as usage_amount,
	   employee_medical.remaining_amount
from employee_medical, employee
where employee_medical.year = ?
      and employee.id = employee_medical.emp_id]]></xml-property>
