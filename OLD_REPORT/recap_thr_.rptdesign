            <xml-property name="queryText"><![CDATA[select employee.nik,
       employee.name,
       employee.department,
       departments.department_name,
       employee.position,
       positions.position_name,
       date_format(employee.join_date, '%d %M %Y') as join_date,
       period_diff(date_format(now(), '%Y%m'), date_format(join_date, '%Y%m')) + 1 as work_period,
       floor((period_diff(date_format(now(), '%Y%m'), date_format(join_date, '%Y%m')) + 1) / 12) as year_work_period,
       ((period_diff(date_format(now(), '%Y%m'), date_format(join_date, '%Y%m')) + 1) % 12) as month_work_period,
       (case
        	when employee_thr.currency = 'IDR' then employee_thr.basic_salary
        	when employee_thr.currency = 'USD' then employee_thr.basic_salary * employee_thr.rate
        end ) as basic_salary,
       employee_thr.thr_amount,
       employee_thr.thr_amount - employee_thr.thr_after_tax as thr_tax,
       employee_thr.thr_after_tax
from employee_thr, employee, departments, positions
where employee_thr.emp_id = employee.id
	  and employee_thr.year = ?
	  and employee.department = departments.id
	  and employee.position = positions.id
order by employee.name]]></xml-property>
