            <xml-property name="queryText"><![CDATA[select b.nik, b.name, b.emp_group, b.emp_group_name, b.ptkp_status, b.ptkp_name, b.position_name,
       b.currency, b.rate, b.real_salary, b.salary, b.position_allowance, b.depnaker_post_all_amount,
       b.ptkp_amount, b.nett_monthly_tax_salary, b.yearly_tax_salary, b.PKP, b.pph_1, b.pph_2, b.pph_3, b.pph_4,
       (b.pph_1 + b.pph_2 + b.pph_3 + b.pph_4) as yearly_pph21, b.gross_salary, (b.pph_1 + b.pph_2 + b.pph_3 + b.pph_4)/12 as pph21,
       (b.gross_salary - ((b.pph_1 + b.pph_2 + b.pph_3 + b.pph_4)/12)) as thp
from(
    select *,
           a.salary+a.position_allowance-a.depnaker_post_all_amount as nett_monthly_tax_salary,
           (a.salary+a.position_allowance-a.depnaker_post_all_amount)*12 as yearly_tax_salary,
           (case
              when ((a.salary+a.position_allowance-a.depnaker_post_all_amount)*12) - a.ptkp_amount <= 0 then 0
              else ((a.salary+a.position_allowance-a.depnaker_post_all_amount)*12) - a.ptkp_amount
            end) as PKP,
           (case 
              when (case
                      when ((a.salary+a.position_allowance-a.depnaker_post_all_amount)*12) - a.ptkp_amount <= 0 then 0
                      else ((a.salary+a.position_allowance-a.depnaker_post_all_amount)*12) - a.ptkp_amount
                    end) > max_amount_1 then max_amount_1 * (percentage_1/100)
              else (case
                      when ((a.salary+a.position_allowance-a.depnaker_post_all_amount)*12) - a.ptkp_amount <= 0 then 0
                      else ((a.salary+a.position_allowance-a.depnaker_post_all_amount)*12) - a.ptkp_amount
                    end) * (percentage_1/100)
            end) as pph_1,
           (case 
              when (case
                      when ((a.salary+a.position_allowance-a.depnaker_post_all_amount)*12) - a.ptkp_amount <= 0 then 0
                      else ((a.salary+a.position_allowance-a.depnaker_post_all_amount)*12) - a.ptkp_amount
                    end) - max_amount_1 >= (max_amount_2 - max_amount_1) then (max_amount_2 - max_amount_1) * (percentage_2/100)
              when (case
                      when ((a.salary+a.position_allowance-a.depnaker_post_all_amount)*12) - a.ptkp_amount <= 0 then 0
                      else ((a.salary+a.position_allowance-a.depnaker_post_all_amount)*12) - a.ptkp_amount
                    end) - max_amount_1 > 0 and 
                   (case
                      when ((a.salary+a.position_allowance-a.depnaker_post_all_amount)*12) - a.ptkp_amount <= 0 then 0
                      else ((a.salary+a.position_allowance-a.depnaker_post_all_amount)*12) - a.ptkp_amount
                    end) - max_amount_1 < (max_amount_2 - max_amount_1) then ((case
                                                     when ((a.salary+a.position_allowance-a.depnaker_post_all_amount)*12) - a.ptkp_amount <= 0 then 0
                                                     else ((a.salary+a.position_allowance-a.depnaker_post_all_amount)*12) - a.ptkp_amount
                                                   end) - max_amount_1) * (percentage_2/100)
              else 0
            end) as pph_2,
           (case 
              when (case
                      when ((a.salary+a.position_allowance-a.depnaker_post_all_amount)*12) - a.ptkp_amount <= 0 then 0
                      else ((a.salary+a.position_allowance-a.depnaker_post_all_amount)*12) - a.ptkp_amount
                    end) - max_amount_2 >= (max_amount_3 - max_amount_2) then (max_amount_3 - max_amount_2) * (percentage_3/100)
              when (case
                      when ((a.salary+a.position_allowance-a.depnaker_post_all_amount)*12) - a.ptkp_amount <= 0 then 0
                      else ((a.salary+a.position_allowance-a.depnaker_post_all_amount)*12) - a.ptkp_amount
                    end) - max_amount_2 > 0 and 
                   (case
                      when ((a.salary+a.position_allowance-a.depnaker_post_all_amount)*12) - a.ptkp_amount <= 0 then 0
                      else ((a.salary+a.position_allowance-a.depnaker_post_all_amount)*12) - a.ptkp_amount
                    end) - max_amount_2 < (max_amount_3 - max_amount_2) then ((case
                                                     when ((a.salary+a.position_allowance-a.depnaker_post_all_amount)*12) - a.ptkp_amount <= 0 then 0
                                                     else ((a.salary+a.position_allowance-a.depnaker_post_all_amount)*12) - a.ptkp_amount
                                                   end) - max_amount_2) * (percentage_3/100)
              else 0
            end) as pph_3,
           (case
              when (case
                      when ((a.salary+a.position_allowance-a.depnaker_post_all_amount)*12) - a.ptkp_amount <= 0 then 0
                      else ((a.salary+a.position_allowance-a.depnaker_post_all_amount)*12) - a.ptkp_amount
                    end) - max_amount_3 > 0 then ((case
                                                     when ((a.salary+a.position_allowance-a.depnaker_post_all_amount)*12) - a.ptkp_amount <= 0 then 0
                                                     else ((a.salary+a.position_allowance-a.depnaker_post_all_amount)*12) - a.ptkp_amount
                                                   end) - max_amount_3) * (percentage_4/100)
              else 0
            end) as pph_4,
            a.salary+a.position_allowance as gross_salary
    from(
        select employee.nik,
        	   employee.name, 
        	   employee.department, 
        	   employee.position, 
        	   employee_payment.ptkp_status, 
        	   employee_payment.rate,
        	   ifnull((case 
        		   		when employee_payment.currency = 'IDR' then employee_payment.salary
        		   		when employee_payment.currency = 'USD' then employee_payment.salary * employee_payment.rate
        		   		else employee_payment.salary
        	   end), 0) as salary,
        	   employee_payment.currency,
        	   employee_payment.salary as real_salary,
        	   ifnull(pos_allowance.amount, 0) as position_allowance,
             employee_payment.ptkp_amount,
             
             (case 
                when
                     (select (percentage/100) * ((case 
                                            		   		when employee_payment.currency = 'IDR' then employee_payment.salary
                                            		   		when employee_payment.currency = 'USD' then employee_payment.salary * employee_payment.rate
                                            		   		else employee_payment.salary
                                            	   end) + ifnull(pos_allowance.amount, 0))
                      from depnaker_position_allowance
                      where str_to_date(concat(?,'-',?),'%m-%Y') between depnaker_position_allowance.start_date and depnaker_position_allowance.end_date
                      ) >
                      (select max_value
                       from depnaker_position_allowance
                       where str_to_date(concat(?,'-',?),'%m-%Y') between depnaker_position_allowance.start_date and depnaker_position_allowance.end_date
                      ) then (select max_value
                              from depnaker_position_allowance
                              where str_to_date(concat(?,'-',?),'%m-%Y') between depnaker_position_allowance.start_date and depnaker_position_allowance.end_date
                             )
                else (select (percentage/100) * ((case 
                                            		   		when employee_payment.currency = 'IDR' then employee_payment.salary
                                            		   		when employee_payment.currency = 'USD' then employee_payment.salary * employee_payment.rate
                                            		   		else employee_payment.salary
                                            	   end) + ifnull(pos_allowance.amount, 0))
                      from depnaker_position_allowance
                      where str_to_date(concat(?,'-',?),'%m-%Y') between depnaker_position_allowance.start_date and depnaker_position_allowance.end_date
                      )
             end)
             
             as depnaker_post_all_amount,
        	   dept.department_name, pos.position_name,
        	   ptkp.name as ptkp_name,
        	   employee.emp_group,
        	   employee_group.name as emp_group_name,
             (select max_amount
              from depnaker_pph21_detail
              where depnaker_pph21_detail.depnaker_pph21_id = depnaker_pph21.id
                    and depnaker_pph21_detail.sequence = 1) as max_amount_1,
             (select max_amount
              from depnaker_pph21_detail
              where depnaker_pph21_detail.depnaker_pph21_id = depnaker_pph21.id
                    and depnaker_pph21_detail.sequence = 2) as max_amount_2,
             (select max_amount
              from depnaker_pph21_detail
              where depnaker_pph21_detail.depnaker_pph21_id = depnaker_pph21.id
                    and depnaker_pph21_detail.sequence = 3) as max_amount_3,
             (select max_amount
              from depnaker_pph21_detail
              where depnaker_pph21_detail.depnaker_pph21_id = depnaker_pph21.id
                    and depnaker_pph21_detail.sequence = 4) as max_amount_4,
             (select percentage
              from depnaker_pph21_detail
              where depnaker_pph21_detail.depnaker_pph21_id = depnaker_pph21.id
                    and depnaker_pph21_detail.sequence = 1) as percentage_1,
             (select percentage
              from depnaker_pph21_detail
              where depnaker_pph21_detail.depnaker_pph21_id = depnaker_pph21.id
                    and depnaker_pph21_detail.sequence = 2) as percentage_2,
             (select percentage
              from depnaker_pph21_detail
              where depnaker_pph21_detail.depnaker_pph21_id = depnaker_pph21.id
                    and depnaker_pph21_detail.sequence = 3) as percentage_3,
             (select percentage
              from depnaker_pph21_detail
              where depnaker_pph21_detail.depnaker_pph21_id = depnaker_pph21.id
                    and depnaker_pph21_detail.sequence = 4) as percentage_4
        from employee join employee_payment on employee_payment.emp_id = employee.id
             left outer join correction_payment as correction on correction.emp_id = employee.id and correction.month = ? and correction.year = ?
             left outer join payment_allowance as pos_allowance on employee_payment.id = pos_allowance.payment_id and pos_allowance.pymt_allowance_type = 300
             left outer join employee_ot_summary as ot_regular on ot_regular.emp_id = employee.id and ot_regular.month = ? and ot_regular.year = ? and ot_regular.ot_summary_type = 0
             left outer join employee_ot_summary as ot_holiday on ot_holiday.emp_id = employee.id and ot_holiday.month = ? and ot_holiday.year = ? and ot_holiday.ot_summary_type = 1
             left outer join employee_ot_summary as ot_mt on ot_mt.emp_id = employee.id and ot_mt.month = ? and ot_mt.year = ? and ot_mt.ot_summary_type = 2
             join departments as dept on employee.department = dept.id
             join positions as pos on employee.position = pos.id
             join master_data as ptkp on employee_payment.ptkp_status = ptkp.value and ptkp.type_id = 13
             join master_data as employee_group on employee.emp_group = employee_group.value and employee_group.type_id = 21
             left outer join employee_deduction on employee_payment.id = employee_deduction.payment_id and employee_deduction.type = 0
             join depnaker_pph21 on str_to_date(concat(?,'-',?),'%m-%Y') between depnaker_pph21.start_date and depnaker_pph21.end_date
        where employee_payment.month = ? and employee_payment.year = ?
        order by employee.name) as a
) as b]]></xml-property>
